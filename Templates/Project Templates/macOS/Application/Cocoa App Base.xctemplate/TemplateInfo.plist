<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Kind</key>
    <string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
    <key>Identifier</key>
    <string>com.apple.dt.unit.cocoaApplicationBase</string>
    <key>Ancestors</key>
    <array>
        <string>com.apple.dt.unit.applicationBase</string>
        <string>com.apple.dt.unit.osxBase</string>
    </array>
    <key>Targets</key>
    <array>
        <dict>
            <key>TargetIdentifier</key>
            <string>com.apple.dt.cocoaApplicationTarget</string>
            <key>SharedSettings</key>
            <dict>
                <key>COMBINE_HIDPI_IMAGES</key>
                <string>YES</string>
                <key>ASSETCATALOG_COMPILER_APPICON_NAME</key>
                <string>AppIcon</string>
                <key>ASSETCATALOG_COMPILER_GLOBAL_ACCENT_COLOR_NAME</key>
                <string>AccentColor</string>
                <key>LD_RUNPATH_SEARCH_PATHS</key>
                <string>$(inherited) @executable_path/../Frameworks</string>
            </dict>
        </dict>
    </array>
    <key>Nodes</key>
    <array>
        <string>Info.plist:DeploymentTarget</string>
        <string>Info.plist:NSHumanReadableCopyright</string>
        <string>Assets.xcassets</string>
     </array>
    <key>Definitions</key>
    <dict>
        <key>Assets.xcassets</key>
        <dict>
            <key>Path</key>
            <string>Assets.xcassets</string>
            <key>AssetGeneration</key>
            <array>
                <dict>
                    <key>Type</key>
                    <string>appicon</string>
                    <key>Name</key>
                    <string>AppIcon</string>
                    <key>Platforms</key>
                    <dict>
                        <key>macOS</key>
                        <string>true</string>
                    </dict>
                </dict>
                <!-- AccentColor is placed statically. -->
            </array>
            <key>SortOrder</key>
            <integer>100</integer>
        </dict>
        <key>Info.plist:DeploymentTarget</key>
        <string><![CDATA[<key>LSMinimumSystemVersion</key>
<string>$(MACOSX_DEPLOYMENT_TARGET)</string>
]]></string>
        <key>Info.plist:MainNib</key>
        <string><![CDATA[<key>NSMainNibFile</key>
<string>MainMenu</string>
]]></string>
        <key>Info.plist:PrincipalClass</key>
        <string><![CDATA[<key>NSPrincipalClass</key>
<string>NSApplication</string>
]]></string>
        <key>Info.plist:Icon</key>
        <string><![CDATA[<key>CFBundleIconFile</key>
<string></string>
]]></string>
        <!-- This is defined globally for templates that don't want language choice -->
        <key>main.m:main:NSApplicationMain</key>
        <string>@autoreleasepool {
    // Setup code that might create autoreleased objects goes here.
}
return NSApplicationMain(argc, argv);</string>
    </dict>
    <key>Options</key>
    <array>
        <dict>
            <key>Identifier</key>
            <string>appLifecycle</string>
            <key>Units</key>
            <dict>
                <key>Cocoa</key>
                <dict>
                    <key>Nodes</key>
                    <array>
                        <string>Info.plist:Icon</string>
                        <string>Info.plist:MainNib</string>
                        <string>Info.plist:PrincipalClass</string>
                    </array>
                </dict>
            </dict>
        </dict>
    </array>
</dict>
</plist>
