<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Kind</key>
    <string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
    <key>Identifier</key>
    <string>com.apple.dt.unit.driverkitFramework</string>
    <key>Ancestors</key>
    <array>
        <string>com.apple.dt.unit.frameworkBase</string>
        <string>com.apple.dt.unit.driverKitBase</string>
    </array>
    <key>Concrete</key>
    <true/>
    <key>Description</key>
    <string>This template creates a framework that uses DriverKit.</string>
    <key>SortOrder</key>
    <integer>1</integer>
    <key>AssociatedTargetSpecification</key>
    <dict>
        <key>PopUpTitle</key>
        <string>Embed in Driver:</string>
        <key>PopUpDescription</key>
        <string>The driver target that will host this new framework. The driver will be set up to link to and embed the new framework.</string>
        <key>AllowableProductTypes</key>
        <array>
            <string>com.apple.product-type.driver-extension</string>
        </array>
        <key>AllowablePlatforms</key>
        <array>
            <string>com.apple.platform.macosx</string>
        </array>
        <key>AssociatedTargetIsDependent</key>
        <true/>
        <key>AssociatedTargetNeedsProductBuildPhaseInjection</key>
        <true/>
    </dict>
    <key>Targets</key>
    <array>
        <dict>
            <key>TargetIdentifier</key>
            <string>com.apple.dt.macosLibraryOrFrameworkTarget</string>
            <key>SharedSettings</key>
            <dict>
                <key>LD_RUNPATH_SEARCH_PATHS</key>
                <string>$(inherited) @executable_path/../Frameworks @loader_path/Frameworks</string>
                <key>FRAMEWORK_VERSION</key>
                <string>A</string>
                <key>COMBINE_HIDPI_IMAGES</key>
                <string>YES</string>
            </dict>
        </dict>
    </array>
    <key>Definitions</key>
    <dict>
        <key>___TARGETNAME:c99ExtIdentifier___.h</key>
        <dict>
            <key>TargetIdentifiers</key>
            <array>
                <string>com.apple.dt.macosLibraryOrFrameworkTarget</string>
            </array>
            <key>BuildAttributes</key>
            <array>
                <string>Public</string>
            </array>
        </dict>
    </dict>
</dict>
</plist>
