<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
	<key>Identifier</key>
	<string>com.apple.dt.unit.instrumentspackage</string>
	<key>Ancestors</key>
	<array>
		<string>com.apple.dt.unit.base_Options</string>
	</array>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.macosx</string>
	</array>
	<key>Concrete</key>
	<true/>
	<key>Description</key>
	<string>This template builds an Instruments package.</string>
	<key>Targets</key>
	<array>
		<dict>
			<key>ProductType</key>
			<string>com.apple.product-type.instruments-package</string>
			<key>TargetIdentifier</key>
			<string>com.apple.dt.instrumentPackageTarget</string>
			<key>SharedSettings</key>
			<dict>
				<key>INSTALL_PATH</key>
				<string>$(LOCAL_LIBRARY_DIR)/Instruments/Packages</string>
				<key>PRODUCT_NAME</key>
				<string>$(TARGET_NAME)</string>
				<key>VERSIONING_SYSTEM</key>
				<string></string>
				<key>ALWAYS_SEARCH_USER_PATHS</key>
				<string>NO</string>
			</dict>
			<key>BuildPhases</key>
			<array>
				<dict>
					<key>Class</key>
					<string>Sources</string>
				</dict>
			</array>
		</dict>
	</array>
	<key>Nodes</key>
	<array>
		<string>___PACKAGENAME___.instrpkg</string>
	</array>
	<key>Definitions</key>
	<dict>
		<key>___PACKAGENAME___.instrpkg</key>
		<dict>
			<key>Path</key>
			<string>___PACKAGENAME___.instrpkg</string>
			<key>TargetIdentifiers</key>
			<array>
				<string>com.apple.dt.instrumentPackageTarget</string>
			</array>
			<key>SubstituteMacros</key>
			<true/>
