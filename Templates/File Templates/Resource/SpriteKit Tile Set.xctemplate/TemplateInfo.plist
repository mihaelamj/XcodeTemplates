<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>com.apple.spritekit.serialized</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>MyTileSet</string>
	<key>Description</key>
	<string>A tile set template for SpriteKit particle emitter.</string>
	<key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.sks</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Default</key>
			<string>Grid Tile Set</string>
			<key>Description</key>
			<string>What tile set to create in the new file</string>
			<key>Identifier</key>
			<string>tilesetSubclass</string>
			<key>Name</key>
			<string>Tile Set template</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>popup</string>
			<key>Values</key>
			<array>
				<string>Grid Tile Set</string>
				<string>Isometric Tile Set</string>
				<string>Hexagonal Tile Set</string>
				<string>Empty Tile Set</string>
			</array>
		</dict>
	</array>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.macosx</string>
		<string>com.apple.platform.iphoneos</string>
		<string>com.apple.platform.appletvos</string>
		<string>com.apple.platform.watchos</string>
	</array>
	<key>Summary</key>
	<string>A tile set template for SpriteKit.</string>
</dict>
</plist>
